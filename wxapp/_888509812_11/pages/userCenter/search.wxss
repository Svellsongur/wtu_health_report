.container {
    background: #fff;
    border-top: 1px solid #ececec;
    margin-top: 92rpx;
}

.clearfix:after {
    content: "";
    display: block;
    clear: both;
}

.title {
    display: flex;
    text-align: center;
    background: #fff;
    padding-top: 20rpx;
    width: 100%;
    z-index: 1001;
    position: fixed;
    top: 0;
}

.title>view {
    width: 50%;
    font-size: 32rpx;
    color: #666;
}

.actived {
    color: #333;
    font-weight: 700;
}

.titBorder {
    width: 70%;
    height: 8rpx;
    background: #438fff;
    margin-left: 15%;
    border-radius: 3rpx;
    margin-top: 14rpx;
}

.name-icon {
    width: 66rpx;
    height: 66rpx;
    line-height: 66rpx;
    text-align: center;
    background: #3888ff;
    display: inline-block;
    border-radius: 50%;
    font-size: 20rpx;
    color: #fff;
    vertical-align: middle;
    margin-right: 22rpx;
    margin-top: -5rpx;
}

.weui-cell {
    border-bottom: 1px solid #ccc;
    padding: 16rpx 32rpx!important;
}

.text-item {
    color: #333;
    font-size: 30rpx;
    line-height: 80rpx;
}

.weui-search-bar {
    background-color: #fff!important;
    padding: 16rpx 32rpx!important;
}

.weui-search-bar__box,.weui-search-bar__label {
    background: #f4f5f7!important;
    border-radius: 40rpx!important;
}

.weui-search-bar__label {
    text-align: left!important;
    padding-left: 30rpx;
}

.zzjg-list {
    padding: 0rpx 32rpx;
    display: flex;
    line-height: 90rpx;
    font-size: 28rpx;
    color: #333;
    border-bottom: 1px solid #ccc;
}

.zzjg-list>view {
    position: relative;
}

.zzjg-list>view:first-child {
    flex: 9;
}

.zzjg-list>view:nth-child(2) {
    flex: 3;
    text-align: right;
}

.mcs-icon,.zzjg-list>view>text {
    display: inline-block;
}

.mcs-icon {
    font-size: 24rpx;
    text-align: center;
}

.zzjg-title {
    line-height: 90rpx;
    font-size: 28rpx;
    padding: 0 32rpx;
    border-bottom: 1px solid #ccc;
}

.color99 {
    color: #999;
}

.color38f {
    color: #3988ff;
}

.zzjg-line {
    margin-right: 20rpx;
    color: #ccc;
}

.zzjg-next {
    margin-left: 20rpx;
    color: #3988ff;
}

.zzjg-icon {
    width: 66rpx;
    height: 66rpx;
    line-height: 66rpx;
    text-align: center;
    font-size: 20rpx;
    color: #fff;
    background: #3888ff;
    border-radius: 50%;
    position: relative;
    top: -5rpx;
    margin-right: 20rpx;
}