.weui-mask {
    background: rgba(0,0,0,.6);
}

.weui-mask,.weui-mask_transparent {
    position: fixed;
    z-index: 1000;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
}

.weui-half-screen-dialog,.weui-mask {
    transition: all .3s;
}

.weui-hidden .weui-mask {
    visibility: hidden;
    opacity: 0;
}

.weui-hidden .weui-half-screen-dialog {
    transform: translateY(100%);
}

.weui-show .weui-mask {
    opacity: 1;
    visibility: visible;
}

.weui-show .weui-half-screen-dialog {
    transform: translateY(0);
}

