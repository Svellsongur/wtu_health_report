.container {
    height: auto;
    background: #fff;
    border-top: 1px solid #ececec;
    padding-top: 82rpx;
    margin-bottom: 60px;
}

.clearfix:after,.tr:after {
    content: "";
    display: block;
    clear: both;
}

.title {
    display: flex;
    flex-direction: row;
    height: 82rpx;
    text-align: center;
    background: #fff;
    border: 1px solid #ececec;
    width: 100%;
    z-index: 1001;
    position: fixed;
    top: 0;
}

.title .title-item {
    flex: 1;
    font-size: 32rpx;
    line-height: 82rpx;
    color: #666;
}

cover-view {
    line-height: 82rpx;
}

.title-item.actived {
    color: #333;
    font-weight: 700;
}

.titBorder {
    width: 70%;
    height: 8rpx;
    background: #438fff;
    margin-left: 15%;
    border-radius: 3rpx;
    margin-top: -8rpx;
}

.rdb-box {
    min-height: calc(100vh - 82rpx);
    padding: 0 20rpx 20rpx;
}

.rdb-tzgg {
    background: #ececec;
}

.tzgg-header {
    padding: 40rpx 0 30rpx;
    position: relative;
    height: 132rpx;
}

.rdb-tzgg-btn {
    padding: 10rpx 28rpx;
    position: absolute;
    background: #fff;
    left: 50%;
    transform: translateX(-50%);
    border-radius: 34rpx;
    text-align: center;
    font-size: 28rpx;
    color: #333;
}

.rdb-tzgg-btn-icon {
    width: 30rpx;
    position: relative;
    top: 6rpx;
    height: 30rpx;
    display: inline-block;
}

.tzgg-body {
    padding: 0 10rpx 10rpx;
}

.tzgg-list {
    margin-bottom: 32rpx;
    background: #fff;
    border-radius: 12rpx;
    padding: 40rpx 40rpx 0;
}

.tzgg-list-header {
    position: relative;
    display: flex;
    margin-bottom: 40rpx;
}

.tzgg-list-icon {
    position: absolute;
    top: 12rpx;
    left: -20rpx;
    width: 12rpx;
    height: 12rpx;
    border-radius: 50%;
    background: #438fff;
}

.tzgg-list-title {
    flex: 7;
    font-size: 32rpx;
    line-height: 32rpx;
    color: #333;
    font-weight: 700;
}

.tzgg-list-time {
    flex: 5;
    line-height: 32rpx;
    font-size: 24rpx;
    color: #666;
    text-align: right;
}

.tzgg-list-body {
    font-size: 28rpx;
    color: #666;
    padding-bottom: 40rpx;
    border-bottom: 1px solid #eee;
}

.tzgg-list-footer {
    padding-top: 32rpx;
    display: flex;
    padding-bottom: 40rpx;
}

.tzgg-list-footer view {
    font-size: 28rpx;
    color: #333;
    flex: 6;
}

.tzgg-list-footer view:nth-child(2) {
    color: #999;
    text-align: right;
}

