.sptg-dilog {
    position: fixed;
    top: 82rpx;
    z-index: 999;
    border-top: 1px solid #ececec;
}

.dilog-bg,.sptg-dilog {
    left: 0;
    bottom: 0;
    right: 0;
}

.dilog-bg {
    position: absolute;
    z-index: 0;
    background: rgba(0,0,0,.5);
    top: 0;
}

.dilog-body {
    background: #fff;
    position: relative;
    z-index: 1;
    width: 100%;
}

.dilog-content {
    width: 100%;
    padding: 0 40rpx 200rpx;
}

.dilog-content>view {
    line-height: 80rpx;
    color: #666;
    font-size: 30rpx;
}

.dilog-content>text {
    font-size: 26rpx;
    color: #666;
    background: #f7f7f9;
    line-height: 70rpx;
    display: inline-block;
    width: calc((100% - 60rpx)/4);
    text-align: center;
    margin: 15rpx 0 15rpx 20rpx;
    border-radius: 4rpx;
}

.dilog-content>text.actived {
    background: #438fff;
    color: #fff;
    font-weight: 400;
}

.dilog-content>text:nth-child(2),.dilog-content>text:nth-child(5n+1) {
    margin-left: 0;
}

.dilog-button {
    display: flex;
}

.dilog-button>view {
    flex: 6;
    line-height: 90rpx;
    font-size: 32rpx;
    border: 1px solid #eee;
    text-align: center;
    color: #666;
}

.dilog-button>view:nth-child(2) {
    background: #438fff;
    color: #fff;
    border-left: none;
    border-color: transparent;
}

.no-record {
    width: 378rpx;
    height: 291rpx;
    margin: 0 auto;
    position: relative;
}

.no-record-text {
    color: #666;
    text-align: center;
}

.main-content-center {
    width: 100%;
    height: 280px;
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    margin: auto;
}