    display: inline-block;
    vertical-align: top;
    margin: 0 8px;
    width: 120px;
}

.weui-icon-btn {
    background-color: transparent;
    background-repeat: no-repeat;
    background-position: 50% 50%;
    background-size: 100%;
    border: 0;
    outline: 0;
