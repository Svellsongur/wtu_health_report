.nav-wrap {
    position: fixed;
    width: 100%;
    top: 0;
    background: #fff;
    color: #323232;
    font-weight: 400;
    z-index: 9999999;
}

.nav-title {
    position: absolute;
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    margin: auto;
    font-size: 36rpx;
    font-weight: 600;
    color: #2c2b2b;
}

.nav-capsule {
    position: relative;
    display: flex;
    align-items: center;
    margin-left: 30rpx;
    width: 140rpx;
    justify-content: space-between;
    height: 100%;
}

.navbar-v-line {
    width: 1px;
    height: 32rpx;
    background-color: #e5e5e5;
}

.back-pre {
    width: 26rpx;
    height: 44rpx;
    margin-top: 8rpx;
}

._navback {
    width: 70rpx;
}

.back-home {
    width: 32rpx;
    height: 36rpx;
    margin-top: 4rpx;
    padding: 10rpx;
}

.nav-capsule .back-home {
    width: 36rpx;
    height: 40rpx;
    margin-top: 3rpx;
}

.nav-capsule ._navback {
    width: 70rpx;
}